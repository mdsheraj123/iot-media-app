    <string name="autoframing_speed_movement_hint">"Type: Int, Range: 1–100</string>
    <string name="autoframing_speed_movement_description">Tracking speed. This is just a coefficient. Bigger value means faster tracking. Actual speed is variable and depends on distance to target position.</string>
