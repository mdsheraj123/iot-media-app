    <string name="gpu_transform_draw_debug_description">Enable and disable drawing on debug data on screen.</string>
    <string name="gpu_transform_enable_crop">Enable auto-framing crop</string>
    <string name="gpu_transform_enable_crop_description">Enable and disable GPU crop.</string>
