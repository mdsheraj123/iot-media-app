    <string name="autoframing_max_move_step_hint">Type: Int, Range: 1–100</string>
    <string name="autoframing_max_move_step_description">Maximum tracking speed per frame for movement. This is just a coefficient. Bigger value means faster tracking. Actual value in pixels depends on resolution.</string>
    <string name="autoframing_max_crop_ratio">Max Zoom factor</string>
    <string name="autoframing_max_crop_ratio_hint">Type: Int, Range: 1–100</string>
    <string name="autoframing_max_crop_ratio_description">Maximum crop/zoom-in ratio.</string>
