    <string name="autoframing_filter_size_hint">Type: Int, Range: 10–60</string>
    <string name="autoframing_filter_size_description">AFR filter debt in frames. Bigger size improve stabilization but it increase response time.</string>
    <string name="autoframing_pos_threshold">Pos Threshold</string>
    <string name="autoframing_pos_threshold_hint">Type: Int, Range: 1–30</string>
    <string name="autoframing_pos_threshold_description">Minimal movement in percent before AFR update.</string>
    <string name="autoframing_size_threshold">Size Threshold</string>
    <string name="autoframing_size_threshold_hint">Type: Int, Range: 1–30</string>
    <string name="autoframing_size_threshold_description">Minimal FOV change in percent before AFR update. </string>
