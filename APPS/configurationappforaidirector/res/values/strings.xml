    <string name="postprocess_yolov5_conf_threshold_description">YoloV5: Confidence threshold</string>
    <string name="postprocess_yolov5_conf_threshold_person">Person confidence</string>
    <string name="postprocess_yolov5_conf_threshold_person_hint">Type: Float, Range: 0.0–1.0</string>
